<Window x:Class="JsonEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:jsonViewer="clr-namespace:JsonEditor.JsonViewer"
        mc:Ignorable="d"
        Title="JSON TreeView" Height="600" Width="900">
    <DockPanel>
        <TextBlock Name="TextBlock" DockPanel.Dock="Top"></TextBlock>
        <jsonViewer:JsonViewer x:Name="JsonViewer" DockPanel.Dock="Bottom"/>
    </DockPanel>


</Window>
